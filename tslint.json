{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "no-debugger": [
      false
    ],
    "no-console": [
      false
    ],
    "jsx-no-lambda": [
      false
    ],
    "interface-name": [
      false
    ],
    "curly": [
      false
    ]
  }
}