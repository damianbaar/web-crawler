{
  "compilerOptions": {
    "resolveJsonModule": true,
    "keyofStringsOnly": true,
    "target": "es6",
    "module": "esnext",
    "declaration": true,
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": true,
    "lib": [
      "dom",
      "esnext"
    ],
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true
  }
}